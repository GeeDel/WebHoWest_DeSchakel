
@model GenreListViewmodel
            @foreach(var genre in Model.Genres)
            { 
                <a asp-area="Home" asp-controller="Home" asp-action="SetGenre" asp-route-id="@genre.Id"
                   class="btn btn-outline-dark"  type="submit">
                <i class="bi-cart-fill me-1"></i>
                @genre.Name
            </a>
            }
